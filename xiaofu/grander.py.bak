import random
for i in range(1,4):
	ass = open('input'+str(i)+'.txt','w')
	n = 10
	m = 30
	ass.write("10 30\n")
	for i in range(0,n):
		for j in range(0,m):
			ass.write(str(random.randint(1,pow(10,500)))+ ' ')
		ass.write('\n')
	ass.close()
